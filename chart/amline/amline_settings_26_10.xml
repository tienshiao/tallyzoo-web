<settings> 
<add_time_stamp>true</add_time_stamp>
  <font>Tahoma</font> 
  <hide_bullets_count>18</hide_bullets_count>     
   <decimals_separator>.</decimals_separator>
   <strings>
   <error_in_data_file>Oops we cannot find any data for creating your chart.</error_in_data_file>
   </strings>
   <connect>true</connect> 
   <background>
    <border_color>#000000</border_color>
    <border_alpha>0</border_alpha>
   </background>
     
  <plot_area>   
    <margins>   
      <left>50</left> 
      <top>60</top>   
      <right>40</right>  
      <bottom>65</bottom> 
    </margins>
  </plot_area>
  
  <grid>       
    <x>                                                             
      <alpha>25</alpha> 
      <approx_count>9</approx_count> 
    </x>
    <y_left>      
      <alpha>25</alpha>   
    </y_left>
  </grid>
  
  <axes>                              
    <x>                               
      <color>#CCCCCC</color>          
    </x>
    <y_left>   
      <color>#CCCCCC</color>  
      <width>1</width>  
    </y_left>
  </axes>  
   <values>
    <x>
      <frequency>1</frequency>
    </x>
  </values>
  <indicator>           
    <color>#0D8ECF</color>
    <line_alpha>50</line_alpha>
    <selection_color>#0D8ECF</selection_color>
    <selection_alpha>20</selection_alpha>     
    <x_balloon_text_color>#FFFFFF</x_balloon_text_color>
  </indicator>
    
 
  
  <zoom_out_button>
    <text_color_hover>#FF0F00</text_color_hover>      
  </zoom_out_button> 
   
     
 
</settings>

<?xml version="1.0" encoding="UTF-8"?>
<settings> 
<add_time_stamp>true</add_time_stamp>
  <data_type>xml</data_type>             
  <font>Tahoma</font>                    
                                                          
  <pie>
     <link_target>_blank</link_target>
     <y>50%</y>
     <start_angle>0</start_angle>
	 <radius>100</radius>
	 <inner_radius>10</inner_radius>
  </pie>
  <strings>
   <error_in_data_file>Oops we cannot find any data for creating your chart.</error_in_data_file>
   <no_data>Oops we cannot find any data for creating your chart.</no_data> 
  </strings>
 <decimals_separator>.</decimals_separator>
  <animation>
    <start_time>0</start_time>           
    <start_effect>regular</start_effect>  
	 <start_alpha>0</start_alpha>
    <pull_out_time></pull_out_time>   
	<pull_out_on_click>false</pull_out_on_click> 
    <pull_out_effect>false</pull_out_effect>   
  </animation>
  
  <data_labels>
    <show>
       <![CDATA[{title}: {percents}%]]>         
    </show>

  </data_labels>

  
  <balloon>                                           
    <show>
       <![CDATA[{title}: {value} ({percents}%)<br>{description}]]>
    </show>
    <max_width>1000</max_width>
    <text_color>000000</text_color>
    <corner_radius>4</corner_radius>
    <border_width>3</border_width>
    <border_alpha>0</border_alpha>
    <border_color>#000000</border_color>
    <alpha>80</alpha>    
  </balloon>
    
  <legend>                                            
    <enabled>true</enabled>                          
	<spacing>1</spacing>
  </legend>  
  
  <labels>                                            
    <label>
	<spacing>10</spacing>
    <margins>10</margins>
       <align>center</align>                             
      
    </label>
  </labels>
</settings>

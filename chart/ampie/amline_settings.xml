<settings> 
<strict_min_max>true</strict_min_max>
<add_time_stamp>true</add_time_stamp>
  <font>Tahoma</font>
  <text_color>333333</text_color>
   <decimals_separator>.</decimals_separator>
   <strings>
   <error_in_data_file>Oops we cannot find any data for creating your chart.</error_in_data_file>
   </strings>
   <connect>true</connect> 

   <background>
    <alpha>90</alpha>
    <border_alpha>0</border_alpha>
  </background>
   
  <plot_area>   
    <margins>
      <left>30</left>
      <right>30</right>
      <top>30</top>
      <bottom>50</bottom>
    </margins>
  </plot_area>
  
  <grid>
    <x>
      <enabled>0</enabled>
      <alpha>10</alpha>
      <approx_count>9</approx_count>
    </x>
    <y_left>
      <alpha>7</alpha>
    </y_left>
  </grid>
  
  <axes>
    <x>
      <tick_length>4</tick_length>
      <width>1</width>
      <color>999999</color>
    </x>
    <y_left>
      <tick_length>4</tick_length>
      <width>1</width>
      <color>999999</color>
    </y_left>
    <y_right>
      <tick_length>4</tick_length>
      <width>1</width>
      <color>999999</color>
    </y_right>
  </axes> 
   <values>
    <x>
      <frequency>1</frequency>
    </x>
  </values>
 <indicator>
    <color>0D8ECF</color>
    <x_balloon_text_color>FFFFFF</x_balloon_text_color>
    <line_alpha>50</line_alpha>
    <selection_color>0D8ECF</selection_color>
    <selection_alpha>20</selection_alpha>
  </indicator>
    
 <scroller>
    <height>8</height>
    <color>FF0000</color>
    <alpha>20</alpha>
    <bg_color>FFFFFF</bg_color>
  </scroller>
  
  <zoom_out_button>
    <text_color_hover>FF0F00</text_color_hover>
  </zoom_out_button>
   

 
</settings>